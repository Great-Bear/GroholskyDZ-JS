
@{
    ViewData["Title"] = "TableProduct";
}
@Html.Partial(@"Partial\Header")

<table id="DataTable">
    <tr id="NameColums">
        <td>ID</td>
        <td>Name</td>
        <td>Price</td>
        <td>CreatedDate</td>
    </tr>
    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>@Model[i].ID</td>
            <td>@Model[i].Name</td>
            <td>@Model[i].Price</td>
            <td>@Model[i].CreatedDate</td>
        </tr>
    }
</table>
@Html.Partial(@"Partial\Footer")

